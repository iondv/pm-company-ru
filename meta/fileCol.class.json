      "semantic": null,
      "selConditions": [],
      "selSorting": [],
      "selectionProvider": null,
      "indexSearch": false,
      "eagerLoading": false,
      "formula": {
        "if": [
          {
            "or": [
              {"eq": [ "$basicObj.status", "new"]},
              {"eq": [ "$basicObj.state", "new"]},
              {"eq": [ "$gosProgramm.status", "new"]},
              {"eq": [ "$contract.stateContr", "new"]},
              {"eq": [ "$briefcase.status", "new"]}
            ]
          },
          "Создан(а)",
          {
            "if": [
              {"eq": [ "$basicObj.status", "create"]},
              "Планирование",
              {
                "if": [
                  {
                    "or": [
                      {"eq": [ "$basicObj.status", "planningVPO"]},
                      {"eq": [ "$basicObj.state", "approvalVPO"]}
                    ]
                  },
                  "Утверждение/согласование ведомственным проектным офисом",
                  {
                    "if": [
                      {
                        "or": [
                          {"eq": [ "$basicObj.status", "planningAdmin"]},
                          {"eq": [ "$basicObj.state", "approvalAdmin"]}
                        ]
                      },
                      "Утверждение/согласование администратором",
                      {
                        "if": [
                          {
                            "or": [
                              {"eq": [ "$basicObj.status", "planningHead"]},
                              {"eq": [ "$basicObj.state", "approvalHead"]},
                              {"eq": [ "$basicObj.state", "planningHead"]}
                            ]
                          },
                          "Утверждение/согласование руководителем",
                          {
                            "if": [
                              {
                                "or": [
                                  {"eq": [ "$basicObj.status", "planningCurator"]},
                                  {"eq": [ "$basicObj.state", "approvalCurator"]},
                                  {"eq": [ "$basicObj.state", "planningCurator"]}
                                ]
                              },
                              "Утверждение/согласование куратором",
                              {
                                "if": [
                                  {
                                    "or": [
                                      {"eq": [ "$basicObj.state", "work"]},
                                      {"eq": [ "$gosProgramm.status", "work"]},
                                      {"eq": [ "$contract.stateContr", "exec"]},
                                      {"eq": [ "$briefcase.status", "work"]}
                                    ]
                                  },
                                  "В работе/Исполняется",
                                  {
                                    "if": [
                                      {
                                        "or": [
                                          {"eq": [ "$basicObj.status", "inaccepted"]},
                                          {"eq": [ "$basicObj.state", "accept"]},
                                          {"eq": [ "$basicObj.state", "inaccepted"]}
                                        ]
                                      },
                                      "Утвержден(о)",
                                      {
                                        "if": [
                                          {
                                            "or": [
                                              {"eq": [ "$basicObj.status", "rework"]},
                                              {"eq": [ "$basicObj.state", "edit"]},
                                              {"eq": [ "$gosProgramm.status", "edit"]},
                                              {"eq": [ "$briefcase.status", "edit"]}
                                            ]
                                          },
                                          "Корректировка/Доработка",
                                          "Не указан"
                                        ]
                                      }
                                    ]
                                  }
                                ]
                              }
                            ]
                          }
                        ]
                      }
                    ]
                  }
                ]
              }
            ]
          }
        ]
      }
