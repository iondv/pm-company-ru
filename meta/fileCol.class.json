    {
      "orderNumber": 40,
      "name": "stateFormula",
      "caption": "Статус",
      "type": 1,
      "size": null,
      "decimals": 0,
      "allowedFileTypes": null,
      "maxFileCount": 0,
      "nullable": true,
      "readonly": false,
      "indexed": true,
