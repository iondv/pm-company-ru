<script>
  $('.list-manager').data('<%= column.property %>_render', function(data, type, row, meta){
    if (type === 'display') {
      var colors = {
        'Низкая': 'rhomb-green',
        'Низкое': 'rhomb-green',
        'Средняя': 'rhomb-yellow',
        'Среднее': 'rhomb-yellow',
        'Высокая': 'rhomb-red',
        'Высокое': 'rhomb-red'
      };
      return '<div class="rhomb-wrapper">'+
          '<svg width="15" height="15">'+
          '<rect class="' + colors[data] + '" x="5" y="-5" width="10" height="10" transform="rotate(45)" />'+
          '</svg>' +
          '</div>'
    }
    return data;
  });
</script>
